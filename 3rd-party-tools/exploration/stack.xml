  <depend stack="navigation" /> <!-- move_base, move_base_msgs, navfn, costmap_2d -->
  <depend stack="ros" /> <!-- roslib, std_msgs, roscpp -->
  <depend stack="stage" /> <!-- stage -->
  <depend stack="slam_gmapping" /> <!-- gmapping -->
